package com.gmail.ozerianskyy;

import java.io.File;

public class Main {

	public static void main(String[] args) {
		
		File file1 = new File("a.txt");
		
		File workFolder = new File(".");
		File[] files = workFolder.listFiles();
		for (int i = 0; i < files.length; i++) {
			System.out.println(files[i]);
		}

	}

}
